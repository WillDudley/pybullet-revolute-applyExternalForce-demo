<?xml version="1.0"?>
<robot name="urdf_robot">
    <link name="baseLink">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.1"/>
            <mass value="10"/>
            <inertia ixx=".1" ixy="0" ixz="0" iyy=".1" iyz="0" izz=".1"/>
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.1"/>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="white">
                <color rgba=".9 .9 .9 1"/>
                <specular rgb="11 1 1"/>
            </material>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.1"/>
            <geometry>
                <sphere radius=".05"/>
            </geometry>
        </collision>
    </link>
</robot>
